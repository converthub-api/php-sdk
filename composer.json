{
    "name": "converthub/php-sdk",
    "description": "Official PHP SDK for ConvertHub API v2 - Convert files between 800+ format pairs",
    "keywords": [
        "converthub",
        "file-conversion",
        "api",
        "sdk",
        "php",
        "document-conversion",
        "image-conversion",
        "pdf",
        "converter"
    ],
    "homepage": "https://converthub.com",
    "license": "MIT",
    "authors": [
        {
            "name": "ConvertHub",
            "email": "support@converthub.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ConvertHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConvertHub\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix",
        "analyze": "vendor/bin/phpstan analyze src tests"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/converthub-api/php-sdk/issues",
        "docs": "https://converthub.com/api/docs",
        "email": "support@converthub.com"
    }
}
